{
  "name": "art-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently 'yarn start:client' 'yarn start:server'",
    "start:client": "node start.js",
    "start:server": "netlify-lambda serve ./lambda",
    "build": "yarn build_client && yarn build_server",
    "build_client": "NODE_ENV=production parcel build src/index.html --out-dir dist/client",
    "build_server": "NODE_ENV=production netlify-lambda build ./lambda",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.2",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^4.4.2",
    "concurrently": "^5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "http-proxy-middleware": "^0.20.0",
    "netlify-lambda": "^1.6.3",
    "parcel-bundler": "^1.12.4",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "jest": "^25.1.0",
    "node-fetch": "^2.6.1",
    "nprogress": "^0.2.0",
    "open-color": "^1.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-waypoint": "^9.0.2",
    "styled-components": "^5.0.1"
  }
}
